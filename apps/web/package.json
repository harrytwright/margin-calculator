{
  "name": "@menubook/web",
  "version": "0.3.0",
  "description": "Menu Book marketing landing page",
  "scripts": {
    "predev": "node scripts/build-blog.js",
    "dev": "parcel src/index.html src/blog/index.html src/blog/*.html --open",
    "dev:watch": "node scripts/watch-blog.js & npm run dev",
    "prebuild": "node scripts/build-blog.js",
    "build": "parcel build src/index.html src/blog/index.html src/blog/*.html --public-url /",
    "postbuild": "cp src/robots.txt dist/ && cp src/sitemap.xml dist/",
    "preview": "parcel serve dist/ --open"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.16.3",
    "@parcel/transformer-jsonld": "^2.16.3",
    "@parcel/transformer-postcss": "^2.16.3",
    "@parcel/transformer-webmanifest": "^2.16.3",
    "autoprefixer": "^10.4.23",
    "chokidar": "^4.0.3",
    "gray-matter": "^4.0.3",
    "marked": "^15.0.12",
    "parcel": "^2.16.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19"
  },
  "keywords": [
    "menu-book",
    "landing",
    "marketing"
  ],
  "author": "GoBowling Shipley Lanes",
  "license": "MIT",
  "type": "module"
}
